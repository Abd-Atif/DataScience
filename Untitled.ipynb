    "import numpy as np\n",
    "\n",
    "arr = np.array([[3, 4, 3,2,1], [4, 3, 5,1,1], [3, 5, 3,3,3], [2, 1, 3,3,2], [1, 1, 3,2,3]])\n",
    "x=0\n",
    "for i in range(4):\n",
    "    for j in range(5):\n",
    "        y=arr[i][j]-arr[i+1][j]\n",
    "        y=y*y\n",
    "        x=x+y\n",
    "    print(x)\n",
    "\n",
    "print(arr[1][1])"
